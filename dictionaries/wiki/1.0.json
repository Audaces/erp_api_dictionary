{
  "auth": {
    "endpoint": "http://app.wikisistemas.com.br/api/login",
    "method": "POST",
    "send_as_json": true,
    "bodyparam": {
      "Email": "{erp_user}",
      "Senha": "{erp_password}"
    },
    "queryparam": {},
    "headers": {
      "accept": "application/json",
      "Content-Type": "application/json"
    },
    "output": {
      "keys": {
		"access_token": "retorno.token"
      }
    }
  },
  "finished_product": {
    "endpoint": "http://app.wikisistemas.com.br/api/wiki_v2/produto_get",
    "method": "GET",
    "send_as_json": true,
    "erp_search_id": "id",
    "queryparam": {
      "pagina": 1
    },
    "bodyparam": {},
    "header": {
      "Token": "{erp_user}"
    },
    "output": {
      "result_list_key": "retorno",
      "keys": {
        "prod_emp_id": "ProdEmpId",
        "prod_id": "ProdId",
        "prod_referencia": "ProdReferencia",
        "prod_descricao": "ProdDescricao",
        "prod_descricao2": "ProdDescricao2",
        "prod_situacao": "ProdSituacao",
        "prod_tipo": "ProdTipo",
        "prod_classificacao": "ProdClassificacao",
        "prod_ncm": "ProdNcm",
        "prod_forn_id": "ProdFornId",
        "prod_controla_estoque": "ProdControlaEstoque",
        "prod_estoque_minimo": "ProdEstoqueMinimo",
        "prod_marca_id": "ProdMarcaId",
        "prod_grupo_id": "ProdGrupoId",
        "prod_segmento_id": "ProdSegmentoId",
        "prod_peso_liq": "ProdPesoLiq",
        "prod_peso_bruto": "ProdPesoBruto",
        "prod_und": "ProdUnd",
        "prod_altura": "ProdAltura",
        "prod_largura": "ProdLargura",
        "prod_comprimento": "ProdComprimento",
        "prod_composicao": "ProdComposicao",
        "prod_colecao_id": "ProdColecaoId",
        "prod_modelo_id": "ProdModeloId",
        "prod_linha_id": "ProdLinhaId",
        "prod_categoria_id": "ProdCategoriaId",
        "prod_foto": "ProdFoto",
        "prod_observacao": "ProdObservacao",
        "prod_datacad": "ProdDatacad",
        "prod_usr_id": "ProdUsrId",
        "prod_data_alt": "ProdDataAlt",
        "prod_foto2": "ProdFoto2",
        "prod_codigo_fornecedor": "ProdCodigoFornecedor",
        "prod_kit": "prodKit",
        "prod_controle_grade": "ProdControleGrade",
        "prod_grade_id": "ProdGradeId",
        "prod_sub_grupo_id": "ProdSubGrupoId"
      },
      "custom_keys_list": {
        "Cor": "aditional_cor_custom_keys_list",
        "Grade": "aditional_grade_custom_keys_list",
        "Preco": "aditional_preco_custom_keys_list",
        "Categoria": "aditional_categoria_custom_keys_list",
        "Saldo_Estoque": "aditional_saldo_estoque_custom_keys_list"
      }
    }
  },
  "aditional_cor_custom_keys_list": {
    "endpoint": "http://app.wikisistemas.com.br/api/wiki_v2/cor_get",
    "method": "GET",
    "send_as_json": true,
    "erp_search_id": "CorId",
    "queryparam": {
      "Id": "%REFERENCE_ID%"
    },
    "bodyparam": {},
    "header": {
      "Token": "{erp_user}"
    },
    "output": {
      "result_list_key": "retorno",
      "keys": {
        "uid": "CorId",
        "name": "CorDescricao",
        "date_created": "CorDatacad",
        "company_id": "CorEmpId",
        "user_id": "CorUsrId"
      }
    }
  },
  "aditional_grade_custom_keys_list": {
    "endpoint": "http://app.wikisistemas.com.br/api/wiki_v2/grade_get",
