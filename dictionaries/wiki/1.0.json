    "erp_search_id": "ItemId",
    "queryparam": {
      "Id": "%REFERENCE_ID%"
    },
    "bodyparam": {},
    "header": {
      "Token": "{erp_user}"
    },
    "output": {
      "result_list_key": "retorno[0].Itens",
      "keys": {
        "uid": "ItemId",
        "size": "ItemTamanho"
      },
      "parent_keys": {
        "description": "GradeDescricao",
        "date_created": "GradeDatacad",
        "company_id": "GradeEmpId"
      }
    }
  },
